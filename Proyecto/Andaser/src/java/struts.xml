<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <!-- Configuration for the default package. -->
    <package name="default" extends="struts-default">
        <action name="adminCategoria" class="Actions.AdminCategoria">
            <result>index.jsp</result>
        </action>
        <action name="inicio" class="Actions.Inicio">
            <result>index.jsp</result>
        </action>
        <action name="login" class="Actions.Login">
            <result name="success">galeria.jsp</result>
            <result name="error">#accesoClientes</result>
        </action>
    </package>
</struts>
